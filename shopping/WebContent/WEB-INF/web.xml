<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>IdentityServlet</servlet-name>
        <servlet-class>com.shopping.servlet.IdentityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddItemServlet</servlet-name>
        <servlet-class>com.shopping.servlet.AddItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.shopping.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.shopping.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveServlet</servlet-name>
        <servlet-class>com.shopping.servlet.RemoveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryServlet</servlet-name>
        <servlet-class>com.shopping.servlet.HistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSubOneServlet</servlet-name>
        <servlet-class>com.shopping.servlet.AddSubOneServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajax</servlet-name>
        <servlet-class>com.shopping.servlet.Ajax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CountServlet</servlet-name>
        <servlet-class>com.shopping.servlet.CountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IdentityServlet</servlet-name>
        <url-pattern>/shopping/IdentityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddItemServlet</servlet-name>
        <url-pattern>/shopping/AddItemServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/shopping/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/shopping/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveServlet</servlet-name>
        <url-pattern>/shopping/RemoveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryServlet</servlet-name>
        <url-pattern>/shopping/HistoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSubOneServlet</servlet-name>
        <url-pattern>/shopping/AddSubOneServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajax</servlet-name>
        <url-pattern>/shopping/Ajax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CountServlet</servlet-name>
        <url-pattern>/shopping/CountServlet</url-pattern>
    </servlet-mapping>
</web-app>